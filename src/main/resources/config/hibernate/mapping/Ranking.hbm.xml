<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="com.bezman.model">
    <class name="Ranking" table="ranking">
        <id name="id"></id>
        
        <property name="points" type="double" column="points"></property>
        <property name="xp" type="double" column="xp"></property>

        <one-to-one name="user" class="User"></one-to-one>
    </class>
</hibernate-mapping> 